-loop_limit 1950

-lib work -vhd "zim_main.vhd" 
-lib work -vhd "spi_slave.vhd" 
-lib work -vhd "DDS_AD9837.vhd" 
-lib work -vhd "ADC_MAX31865.vhd" 
-lib work -vhd "ADC_ADS127.vhd" 
-lib work -vhd "ADC_ADS1252u.vhd" 
-lib work -vhd "vdc_gen_clk.vhd" 
-lib work -vhd "zim_pll.vhd" 
-p "F:/SM_Git_Repositories/software-bza-sm/firmware_ICE/zim1.0"

#set result format/file last
-output_edif zim_Implmnt/zim.edf

#set log file
-logfile "zim_Implmnt/zim_lse.log"
